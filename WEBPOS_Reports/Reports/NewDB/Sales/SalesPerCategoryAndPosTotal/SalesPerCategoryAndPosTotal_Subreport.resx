  <data name="customSqlQuery1.Sql" xml:space="preserve">
    <value>-- select OBJECT_NAME(object_id),* from sys.columns where name='hotelid'
-- select OBJECT_NAME(object_id),* from sys.columns where name='ItemCode'

-------------------------------------------------------------------
-- Πωλήσεις Ειδών ανά Κατηγορία και POS - Συνοπτικά - SUB-REPORT --
-------------------------------------------------------------------

  /*
Declare @FromDate Date = '2016-08-01'
      , @ToDate Date = '2017-02-28'
      , @DepartmentId Bigint = 1
  */

SELECT CategoryDescription = a.CategoryDescription
     -- , PosInfoId = a.PosInfoId, PosInfoDescription
     , Total = SUM( a.Total )
     , Net = SUM( a.Net )
     , VatAmount = SUM( a.VatAmount )

FROM (

-- Declare @FromDate Date = '2016-08-01', @ToDate Date = '2017-02-28'

SELECT CategoryDescription = ISNULL( cat.[Description], 'No Category' ) + ' ' + CAST( odi.VatRate AS varchar( 30 ) ) + '%'
     -- , PosInfoId = pif.Id, PosInfoDescription = pif.[Description]
     , VatAmount = odi.VatAmount
     , Total = CASE WHEN IsVoided = 0 AND InvoiceType != 4 THEN odi.Total ELSE 0 END
     , Net = CASE WHEN IsVoided = 0 AND InvoiceType != 4 THEN odi.Net ELSE 0 END

FROM OrderDetailInvoices_View odi 
     INNER JOIN Invoices_View inv on odi.InvoicesId = inv.id AND ISNULL( inv.IsDeleted, 0 ) = 0
     INNER JOIN PosInfo AS pif ON pif.Id = inv.PosInfoId -- AND pif.DepartmentId = @DepartmentId
     LEFT OUTER JOIN ProductCategories pc on odi.productCategoryId = pc.Id
     LEFT OUTER JOIN Categories AS cat ON cat.Id = pc.CategoryId
     INNER JOIN EndOfDay_View eod on inv.EndofDayId = eod.Id
                AND IsNull( CAST( CONVERT( VARCHAR( 10 ), eod.FODay, 120 ) AS DATETIME )
                          , CAST( getdate() AS DATE ) ) BETWEEN @FromDate AND @ToDate
           
WHERE ISNULL( odi.IsDeleted, 0 ) = 0
      AND InvoiceType NOT IN ( 2, 3, 8, 10, 11, 12 )
      -- AND ( ( IsExtra = 0 ) OR ( IsExtra = 1 AND odi.Total &gt; 0 ) )

) a

GROUP BY a.CategoryDescription
       -- , a.CategoryId
       -- , a.PosInfoId
       -- , a.PosInfoDescription</value>
